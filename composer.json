{
    "name": "tcg-webdev/pusher-beams",
    "description": "Pusher Beams is a push notification service from Pusher.",
    "keywords": [
        "laravel",
        "pusher",
        "push-notifications",
        "notification",
        "push",
        "beams"
    ],
    "license": "MIT",
    "homepage": "https://github.com/tcg-webdev/pusher-beams",
    "support": {
        "issues": "https://github.com/tcg-webdev/pusher-beams/issues",
        "source": "https://github.com/tcg-webdev/pusher-beams"
    },
    "authors": [
        {
            "name": "Neo Ighodaro",
            "email": "neo@creativitykills.co"
        },
        {
            "name": "Richard Hyland",
            "email": "rhyland@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/events": "5.5.* || 5.6.* || 5.7.*|| 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/notifications": "5.5.* || 5.6.* || 5.7.*|| 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/queue": "5.5.* || 5.6.*|| 5.7.*|| 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.*|| 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "pusher/pusher-push-notifications": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.6",
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "TcgWebdev\\PusherBeams\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TcgWebdev\\PusherBeams\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TcgWebdev\\PusherBeams\\PusherBeamsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
